<table class="table ">
    <thead>
        <tr>
            <th scope="col">File Name</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        <% p.forEach( function(model) { %>
        <tr>

            <td><%=model.filename %></td>
          
            <td>
                <a class="btn btn-danger btn-sm " href="/file/download/<%= model.id %>" role ="button">download</a>
                
            </td>
        </tr>
        <% }); %>
    </tbody>
</table>

